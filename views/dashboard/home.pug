- pageName = "Dashboard Home"
include ../mixins/header
+header(pageTitle)
body
  include ../mixins/navbar
  +navbar(pageName)
  section.intro(style="min-height:100vh;min-width:100vw;")
    .container
      .row.text-center
        .col-md-12.col-sm-12.col-xs-12
          .heading-count
            img#prof_img(src="/assets/img/transparent_prof.png").img-fluid
            h2 Hackenger Hunt
            a.btn.btn-common#form-submit(href="/team/sign_in" target="_blank") Team Sign In
            a.btn.btn-common#form-submit(href="/team/register" target="_blank") Register Your Team
  include ../mixins/footer
  +footer()
