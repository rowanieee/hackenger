mixin header(pageName)
  doctype html
  html(lang='en')
    head
      // Required meta tags
      meta(content="text/html;charset=utf-8", http-equiv="Content-Type")
      meta(content="utf-8", http-equiv="encoding")
      meta(name='viewport', content='width=device-width,initial-scale=1,shrink-to-fit=no')
      - fullTitle = (pageTitle ? pageTitle + " | " : "") + "ProfHacks 2019"
      title= fullTitle
      link(rel="shortcut icon", href="/assets/img/favicon.png", type="image/png")

      // Bootstrap CSS
      link(rel='stylesheet', type='text/css', href='/assets/css/bootstrap.min.css')
      // Slicknav
      link(rel='stylesheet', type='text/css', href='/assets/css/slicknav.css')
      // Off Canvas Menu
      link(rel='stylesheet', type='text/css', href='/assets/css/menu_sideslide.css')
      // Color Switcher
      link(rel='stylesheet', type='text/css', href='/assets/css/vegas.min.css')
      // Animate
      link(rel='stylesheet', type='text/css', href='/assets/css/animate.css')
      // Main Style
      link(rel='stylesheet', type='text/css', href='/assets/css/main.css')
      // Responsive Style
      link(rel='stylesheet', type='text/css', href='/assets/css/responsive.css')
      // Sponsor styles
      link(rel='stylesheet', type='text/css', href='/assets/css/sponsors.css')
      // Schedule styles
      link(rel='stylesheet', type='text/css', href='/assets/css/schedule.css')
      // Font Awesome Style
      link(rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.0/css/all.css" integrity="sha384-lZN37f5QGtY3VHgisS14W3ExzMWZxybE1SJSEsQp9S+oqd12jhcu+A56Ebc1zFSJ" crossorigin="anonymous")

      link(href="https://fonts.googleapis.com/css?family=Montserrat:400,700", rel="stylesheet", type="text/css")
      link(href="https://fonts.googleapis.com/css?family=Lato:400,700,400italic,700italic", rel="stylesheet", type="text/css")

      script(src="/assets/js/jquery-min.js")
      script(src="/assets/js/popper.min.js")
      script(src="/assets/js/bootstrap.min.js")
      script(src="/assets/js/vegas.min.js")
      script(src="/assets/js/jquery.countdown.min.js")
      script(src="/assets/js/classie.js")
      script(src="/assets/js/scrolling-nav.js")
      script(src="/assets/js/jquery.nav.js")
      script(src="/assets/js/jquery.easing.min.js")
      script(src="/assets/js/wow.js")
      script(src="/assets/js/jquery.slicknav.js")
      script(src="/assets/js/main.js")
      script(src="/assets/js/form-validator.min.js")
      script(src="/assets/js/contact-form-script.min.js")
      script(src="//cdnjs.cloudflare.com/ajax/libs/jasny-bootstrap/3.1.3/js/jasny-bootstrap.min.js")
      //script(src="assets/js/menu.js")
